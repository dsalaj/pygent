#:kivy 1.0.9

<Field>:
    size: 25, 25
    canvas.before:
        Color:
            rgba: self.r, self.g, self.b,1
        Rectangle:
            pos:self.pos
            size:self.size

<Zombie>:
    size: 25, 25
    canvas:
        Color:
            rgba: 0, 1, 0, 1     
        Ellipse:
            pos: self.pos
            size: self.size
